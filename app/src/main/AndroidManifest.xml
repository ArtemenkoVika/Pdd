<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.pdd">

    <application
        android:allowBackup="true"
        android:icon="@drawable/sign"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainSelectionActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.SignActivity"
            android:label="@string/title_activity_sign" />
        <activity
            android:name=".activity.ReadingActivity"
            android:label="@string/title_activity_reading" />
        <activity
            android:name=".activity.ResultsActivity"
            android:label="@string/title_activity_results" />
        <activity
            android:name=".activity.TestsOfPDDActivity"
            android:label="@string/title_activity_tests_of_pdd"
            android:launchMode="singleTask" />
        <activity
            android:name=".activity.RegistrationActivity"
            android:label="@string/title_activity_registration" />
    </application>

</manifest>
